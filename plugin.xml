<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="uece.ees"
         point="org.eclipse.ui.newWizards">
      <category
            id="UefiIde.category.wizards"
            name="Uefi EDK2 Tools">
      </category>
      <wizard
            category="UefiIde.category.wizards"
            class="me.fmartins.unbiosed.wizards.ExistingModuleProjectWizard"
            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            id="UefIde.wizards.new.module"
            name="Existing EDK2 Module Wizard"
            project="true">
      </wizard>
      <wizard
            category="UefiIde.category.wizards"
            class="me.fmartins.unbiosed.wizards.NewEdk2ModuleProjectWizard"
            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            id="UefIde.wizards.new.empty_module"
            name="New EDK2 Module Wizard"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="me.fmartins.unbiosed.startup.SetResourceChangeListenerStartup">
      </startup>
      <startup
            class="me.fmartins.unbiosed.startup.SetDefaultEditorsStartup">
      </startup>
   </extension>

</plugin>
